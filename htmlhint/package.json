{
	"name": "HTMLHint",
	"displayName": "HTMLHint",
	"version": "0.0.1",
	"private": false,
	"description": "VS Code integration for HTMLHint - A Static Code Analysis Tool for HTML",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandHTMLHint#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandHTMLHint/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandHTMLHint.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "node ./node_modules/typescript/bin/tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"htmlhint.documentSelector": {
					"default": [
						"html",
						"htm"
					],
					"description": "The associated document types to be linted.",
					"type": "array"
				},
				"htmlhint.enable": {
					"default": true,
					"description": "Control whether htmlhint is enabled for HTML files or not.",
					"type": "boolean"
				},
				"htmlhint.options": {
					"default": {},
					"description": "The htmlhint options object to provide args to the htmlhint command.",
					"type": "object"
				}
			},
			"title": "HTML-Hint configuration",
			"type": "object"
		}
	},
	"activationEvents": [
		"onLanguage:html"
	],
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "^6.0.41"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "HTMLHint.png",
	"galleryBanner": {
		"color": "#0000FF",
		"theme": "dark"
	}
}
